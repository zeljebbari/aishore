{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['metadata']['url']"
								},
								"sink": {
									"name": "URL",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['metadata']['index_tags']['borrower_first_name']"
								},
								"sink": {
									"name": "First_Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['metadata']['index_tags']['borrower_last_name']"
								},
								"sink": {
									"name": "Last_Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['metadata']['index_tags']['doc_type']"
								},
								"sink": {
									"name": "Document_Type",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['metadata']['index_tags']['timestamp']"
								},
								"sink": {
									"name": "Time",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['key_value_pairs'][0]['key']"
								},
								"sink": {
									"name": "Pair_Key",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['key_value_pairs'][0]['value']"
								},
								"sink": {
									"name": "Pair_Value",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['key_value_pairs'][0]['confidence']"
								},
								"sink": {
									"name": "Confidence",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "$['key_value_pairs'][0]['page_number']"
								},
								"sink": {
									"name": "Page",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['key_value_pairs']"
					}
				},
				"inputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}